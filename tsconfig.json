{
  "compilerOptions": {
    "jsx": "react",
    "outDir": "./dist/",
    "sourceMap": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "module": "es6",
    // "noImplicitAny": true,
    // "noEmit": true,
    // "noFallthroughCasesInSwitch": true,
    // "noImplicitReturns": true,
    // "noUnusedLocals": true,
    // "noUnusedParameters": true,
    // "lib": [],
    "resolveJsonModule": true,
    "paths": {
      // TODO: Fix paths
      "src/*": ["src/*"],
      "renderers/*": ["src/tensor-renderer/*"],
      "*": ["node_modules/*"]
    },
    "strict": true,
    "target": "es5"
    // "types": []
  },
  "exclude": []
}
